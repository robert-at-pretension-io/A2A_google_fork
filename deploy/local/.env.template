# A2A Environment Configuration
# Copy this file to .env and fill in your API keys

# Google AI Studio API Key
GOOGLE_API_KEY=your_api_key_here

# Persistence settings (TRUE/FALSE)
A2A_PERSISTENCE=TRUE

# Vertex AI settings (required for vertex_image_gen agent)
# To use the vertex_image_gen agent, uncomment and fill in these values
# GOOGLE_GENAI_USE_VERTEXAI=TRUE
# GOOGLE_CLOUD_PROJECT=your_project_id
# GOOGLE_CLOUD_LOCATION=us-central1

# ElevenLabs API Key (required for elevenlabs_tts agent)
# ELEVENLABS_API_KEY=your_elevenlabs_key_here

# Port assignments for agents
# These match the ports in docker-compose.yaml and Kubernetes manifests
# google_adk: 10002
# elevenlabs_tts: 10005
# vertex_image_gen: 10006